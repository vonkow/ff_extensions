{
	"name": "Passover",
	"version": "1.0",
	"description": "License detector for JavaScript",
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*","file:///*/*"],
			"js": ["cont.js"]
		}
	],
	"permissions": [
		"http://*/*",
		"tabs"
	]
}
